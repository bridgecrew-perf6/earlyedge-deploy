{
    "domElements": {
        "intro": {
            "introBackground": {
                "background_image": "titleScreen",
                "class": "background1",
                "buttonImage": "playButton",
                "play_button_class": "intro-play-btn vendorPlayerButtonLocal"
            }
        },
        "activityScreen": {
            "background": {
                "image": "activityScreen",
                "class": "background1"
            }, 
            "tensCharLottie": {
                "lottie": "tensCharLottie",
                "class":"tensCharLottie",
                "textId":"#Pink_text",
                "segments": {
                    "idle": [0, 108],
                    "happy": [108, 216],
                    "sad": [217, 301]
                }
            },
            "onesCharLottie": {
                "lottie": "onesCharLottie",
                "class":"onesCharLottie",
                "textId":"#Blue_text",
                "segments": {
                    "idle": [0, 108],
                    "happy": [108, 216],
                    "sad": [217, 301]
                }
            },
            "selectRange": {
                "numberWrapperClass":"letter-wrapper",
                "numberTextBlockClass":"letter-text-block",
                "headingTextClass":"select-range-title",
                "headingText":"Select a number range.",
                "selectBackgroundImage":"selectionScreen",
                "backgroundClass": "background1",
                "nextButton":"nextButton",
                "nextButtonClass": "selection-play-btn vendorPlayerButtonLocal",
                "element": [{
                        "text": "1 - 20",
                        "index":0,
                        "min": 1,
                        "max": 20,
                        "class":"selection-number1"
                    },
                    {
                        "text": "21 - 50",
                        "index":1,
                        "min": 21,
                        "max": 50,
                        "class":"selection-number2"
                    },
                    {
                        "text": "51 - 70",
                        "index":2,
                        "min": 51,
                        "max": 70,
                        "class":"selection-number3"
                    },
                    {
                        "text": "71 - 100",
                        "index":3,
                        "min": 71,
                        "max": 100,
                        "class":"selection-number4"
                    }
                ]
            },
            "questionText": {
                "class": "question-text"
            }, 
            "quitButton": {
                "image":"quitButtonImage",
                "class": "game-close-btn"
            }, 
            "brick": {
                "image": "brickImage",
                "tensBlockClass": "bricks-block",
                "onesBlockClass":"single-bricks"
            },
            "result": {
                "correctImage": "correctImage",
                "correctSignClass":"correct-sign",
                "wrongImage": "wrongImage",
                "wrongSignClass":"wrong-sign",
                "resultScreenImage":"resultScreen"
            }
        },
        "feedBackScreen": {
            "lottieHolder": "star",
            "lottie": "GameEndHappy",
            "ribbonLottieHolder": "ribbonLottiediv",
            "ribbonLottie": "ribbonLottie"
        }
    },
    "numbers": {
        "min": 11,
        "max": 99
    },
    "transition": "transition",
    "transitionClass": "transition",
    "minimumTry": 5
}